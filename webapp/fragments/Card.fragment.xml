<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards">
	<f:Card>
	<f:header>
		<card:Header
			title="{/cardtitle}"
			subtitle="{/cardsubtitle}"
			iconSrc="{/cardimageUrl}"
			iconDisplayShape="Square" />
	</f:header>
	<f:content>
		<l:VerticalLayout class="sapUiContentPadding" width="100%">
			<l:BlockLayout>
				<l:BlockLayoutRow>
					<l:BlockLayoutCell title="Main Information" width="1">
						<HBox class="sapUiTinyMarginBottom">
							<Label text="Supplier:"/>
						</HBox>
						<HBox class="sapUiSmallMarginBottom">
							<Text text="{/cardtitle}" />
						</HBox>
						<HBox class="sapUiTinyMarginBottom">
								<Button text="{/buttitle}" press="onBookPress" type="{/buttype}" />
						</HBox>
					
					</l:BlockLayoutCell>
					
				</l:BlockLayoutRow>
			</l:BlockLayout>
		</l:VerticalLayout>
	</f:content>
</f:Card>
</core:FragmentDefinition>